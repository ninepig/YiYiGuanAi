<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.example.yamengwenjing.yiyiguanai.UIActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="moa analysis"
        android:id="@+id/moa_analysis"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="34dp"
        android:onClick="gotoMoa"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Personal Setting"
        android:id="@+id/Personal_setting"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="personalSetting"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sycn with Cloud"
        android:id="@+id/Sycn_with_cloud"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="sychWithCloud"/>
</LinearLayout>
